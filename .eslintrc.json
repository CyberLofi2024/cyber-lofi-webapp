{
    "extends": [
      "plugin:storybook/recommended",
      "next",
      "next/core-web-vitals",
      "eslint:recommended",
      "plugin:storybook/recommended"
    ],
    "globals": {
      "React": "readonly"
    },
    "overrides": [
      {
        "files": [
          "*.stories.@(ts|tsx|js|jsx|mjs|cjs)"
        ]
      }
    ],
    "rules": {
      "no-unused-vars": "warn"
    }
  }